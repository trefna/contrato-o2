{
  "version": 2,
  "builds": [
    {
      "src": "client/admin-frontend/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "client/admin-frontend/build" }
    },
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/(.*)", "dest": "client/admin-frontend/build/$1" }
  ]
}
